{
  "version": 2,
  "builds": [
    {
      "src": "build_all.sh",
      "use": "@vercel/python"
    },
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "backend/main.py" },
    { "source": "/assets(.*)", "destination": "/frontend/assets/$1" },
    { "source": "/(.*)", "destination": "/frontend/index.html" }
  ]
}
